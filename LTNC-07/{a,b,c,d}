#include <bits/stdc++.h>

using namespace std;


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s = "abcd";
    for(int i = 0; i < s.size(); i++){
        for(int j = 0; j < s.size(); j++){
            for(int k = 0; k < s.size(); k++){
                cout << s[i] << s[j] << s[k] << " ";
            }
        }
    }
    return 0;
}
