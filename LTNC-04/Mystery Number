using namespace std;

int main() {
    int n; cin >> n;
    vector<int> a(n), b(n+1);
    int suma = 0, sumb = 0;
    for(int i = 0; i < n; i++){
        cin >> a[i];
        suma += a[i];
    }
    for(int i = 0; i < n+1; i++){
        cin >> b[i];
        sumb += b[i];
    }
    cout << sumb - suma;
