#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int maxNumber(int a, int b, int c, int d){
    int max = a;
    if(max < b) max = b;
    if(max < c) max = c;
    if(max < d) max = d;
    return max;
}
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,b,c,d;
    cin >> a >> b >> c >> d;
    cout << maxNumber(a,b,c,d);
    return 0;
}
