#include <bits/stdc++.h>

using namespace std;
struct node{
    int data;
    struct node* next;
};

node *makeNode(int x){
    node* newNode = (node*)malloc(sizeof(node));
    newNode->data = x;
    newNode->next = NULL;
    return newNode;
}

void pushFront(node **head,int x){
    node *newNode = makeNode(x);
    newNode->next = (*head);
    (*head) = newNode;
}

int main()
{
    node* head = NULL;
            int x;cin >> x;
            pushFront(&head,x);
        }
       
    return 0;
}
