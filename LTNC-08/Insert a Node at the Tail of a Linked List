#include <bits/stdc++.h>

using namespace std;
struct node{
    int data;
    struct node* next;
};

node *makeNode(int x){
    node* newNode = (node*)malloc(sizeof(node));
    newNode->data = x;
    newNode->next = NULL;
    return newNode;
}

void pushBack(node **head,int x){
    node* newNode = makeNode(x);
    node *tmp =(*head);
    while(tmp->next != NULL){
        tmp = tmp->next;
    }
    tmp->next = newNode;


}
void duyet(node* head){
    while(head != NULL){
        cout << head->data << " ";
        head = head->next;
    }
}

int main()
{
    node* head = NULL;
    int x;cin >>x;
    pushBack(&head,x);      
    return 0;
}
