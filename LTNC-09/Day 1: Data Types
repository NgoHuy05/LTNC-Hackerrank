#include <iostream>
#include <iomanip>
#include <limits>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
    int x;cin >> x;
    double y; cin >> y;
    cin.ignore();
    string a;
    getline(cin,a);
    string b = s + a;
    cout << x + i << endl;
    cout << fixed << setprecision(1) << y+d << endl;
    cout << b;

return 0;
}

